	f2 $f prop_tiny_proofless_wff ch $.
	f3 $f prop_tiny_proofless_wff th $.
	p17 $p ( ( th -> ph ) -> ( ( th -> ps ) -> ( th -> ch ) ) ) $=
	e0 f0 f1 f2 a1 f3 p15 f3 f1 f2 a3 f3 f0 a1 f3 f1 f2 a1 a1 f3 f1 a1 f3 f2 a1 a1 p8 $.
$}

	$(   Inference chaining two syllogisms.   $)
${
	$v ph ps ch th $.
	e0 $e ( ph -> ps ) $.
	e1 $e ( ps -> ch ) $.
	e2 $e ( ch -> th ) $.
	f0 $f prop_tiny_proofless_wff ph $.
	f1 $f prop_tiny_proofless_wff ps $.
	f2 $f prop_tiny_proofless_wff ch $.
	f3 $f prop_tiny_proofless_wff th $.
	p18 $p ( ph -> th ) $=
	e1 e0 e2 f0 f1 f2 f3 p16 f1 f2 a1 f0 f3 a1 a5 $.
$}

